{
  "name": "artgallery",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "events": "^1.0.2",
    "flux": "^2.1.1",
    "lodash": "^3.10.1",
    "object-assign": "^4.0.1",
    "react": "^0.13.3",
    "react-imageloader": "^2.1.0"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "grunt": "^0.4.5",
    "grunt-browserify": "^4.0.0",
    "grunt-contrib-jshint": "^0.11.2",
    "grunt-contrib-uglify": "^0.9.1",
    "jest-cli": "^0.5.0",
    "jshint-stylish": "^2.0.1",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1"
  },
  "jest": {
    "scriptPreprocessor": "__tests__/preprocessor.js",
    "globals": {
      "__APPDIR__": "<rootDir>/src"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/lodash",
      "node_modules/object-assign"
    ]
  },
  "browserify-shim": {
    "manifestArt": "global:manifestArt",
    "manifestInstallations": "global:manifestInstallations"
  },
  "browserify": {
    "transform": [
      "reactify",
      "browserify-shim"
    ]
  }
}
